{
  "name": "@paperclip/workspace-client",
  "version": "0.1.0",
  "description": "Universal TypeScript client for Paperclip workspace service",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./grpc": {
      "types": "./dist/transport/grpc.d.ts",
      "import": "./dist/transport/grpc.js"
    },
    "./grpc-web": {
      "types": "./dist/transport/grpc-web.d.ts",
      "import": "./dist/transport/grpc-web.js"
    },
    "./machine": {
      "types": "./dist/machine/index.d.ts",
      "import": "./dist/machine/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.10.0",
    "@grpc/proto-loader": "^0.7.13",
    "@paperclip/common": "workspace:*",
    "@paperclip/proto": "workspace:*"
  },
  "peerDependencies": {
    "react": "^18.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  },
  "packageManager": "yarn@4.1.0"
}
