{
  "name": "@paperclip-lang/rollup-plugin",
  "version": "0.1.0",
  "description": "Rollup plugin for Paperclip files",
  "packageManager": "yarn@4.1.0",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "watch": "tsup src/index.ts --format cjs,esm --dts --watch",
    "prepublishOnly": "yarn build"
  },
  "keywords": [
    "paperclip",
    "rollup",
    "plugin"
  ],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "rollup": "^2.0.0 || ^3.0.0 || ^4.0.0"
  },
  "dependencies": {
    "@paperclip-lang/wasm": "^0.1.0"
  },
  "devDependencies": {
    "@rollup/pluginutils": "^5.0.0",
    "@types/node": "^20.0.0",
    "rollup": "^4.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "files": [
    "dist",
    "README.md"
  ]
}
