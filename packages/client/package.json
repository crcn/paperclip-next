{
  "name": "paperclip-client",
  "version": "0.1.0",
  "description": "Paperclip browser client for real-time preview",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "vite",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "grpc-client": "node --loader ts-node/esm src/grpc-client.ts",
    "test:latency": "node --loader ts-node/esm src/test-latency-final.ts"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.10.0",
    "@grpc/proto-loader": "^0.7.13",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/jsdom": "^27",
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitest/ui": "^4.0.18",
    "jsdom": "^27.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.11",
    "vitest": "^4.0.18"
  },
  "packageManager": "yarn@4.1.0"
}
